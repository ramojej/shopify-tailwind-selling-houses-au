<div class="minimal-section">
  <h1>{{ section.settings.title }}</h1>
</div>

{% schema %}
{
  "name": "Minimal Section",
  "tag": "section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Section Title",
      "default": "Minimal Section"
    }
  ],
  "presets": [
    {
      "name": "Minimal Section"
    }
  ]
}
{% endschema %}